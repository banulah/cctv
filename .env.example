# Frontend Environment Configuration
# Copy this file to .env for local development overrides

# Backend API URL
# Default: Uses relative path (same host as frontend)
# VITE_API_URL=http://backend:8000

# MediaMTX Streaming Server
# Default: Auto-detects from browser hostname
# VITE_MEDIAMTX_HOST=localhost
# VITE_MEDIAMTX_HTTP_PORT=8888
# VITE_MEDIAMTX_RTSP_PORT=8554

# WebSocket Configuration
# Default: Uses relative path (same host as frontend)
# VITE_WS_URL=ws://backend:8000

# Development Settings
# VITE_DEBUG=false
